{
    "name": "wyrihaximus/html-compress",
    "description": "Compress/minify your HTML",
    "license": "MIT",
    "authors": [
        {
            "name": "Cees-Jan Kiewiet",
            "email": "ceesjank@gmail.com",
            "homepage": "http://wyrihaximus.net/"
        }
    ],
    "keywords": [
        "html",
        "compress"
    ],
    "require": {
        "php": ">=5.4.0",
        "meenie/javascript-packer": "1.1",
        "linkorb/jsmin-php": "1.0.0",
        "patchwork/jsqueeze": "v1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.0",
        "phake/phake": "^1.0",
        "composer/composer": "@dev",
        "dflydev/embedded-composer-core": "@dev",
        "dflydev/embedded-composer-console": "@dev",
        "sculpin/sculpin": "2.*@dev",
        "vectorface/dunit": "^2.0",
        "squizlabs/php_codesniffer": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "WyriHaximus\\HtmlCompress\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WyriHaximus\\HtmlCompress\\Tests\\": "tests/"
        }
    }
}
